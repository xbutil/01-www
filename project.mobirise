          "_name": "title1",
          "_customHTML": "<section class=\"title1\" group=\"Article\" plugins=\"amp-main, amp-selector\">\n    <mbr-parameters>\n    <!--Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n    <header>Show/Hide</header>\n    <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n    <input type=\"checkbox\" title=\"Show Progress Block\" name=\"showProgressBlock\" checked>\n    <input type=\"checkbox\" title=\"Show Tabs Block\" name=\"showTabsBlock\">\n    <header>Progress Block</header>\n    <input type=\"checkbox\" title=\"Show Text\" name=\"showProgressText\" checked condition=\"showProgressBlock\">\n    <input type=\"range\" title=\"Progress Count\" name=\"progressAmount\" min=\"1\" max=\"5\" value=\"4\" condition=\"showProgressBlock\">\n    <input type=\"range\" inline title=\"Progress 1\" name=\"progress1\" min=\"0\" max=\"100\" step=\"1\" value=\"60\" condition=\"showProgressBlock\">\n    <input type=\"color\" title=\"Progress 1 Color\" name=\"progress1Color\" value=\"#cd201f\" condition=\"showProgressBlock\">\n    <!--  -->\n    <input type=\"range\" inline title=\"Progress 2\" name=\"progress2\" min=\"0\" max=\"100\" step=\"1\" value=\"100\" condition=\"progressAmount > 1 &amp;&amp; showProgressBlock\">\n    <input type=\"color\" title=\"Progress 2 Color\" name=\"progress2Color\" value=\"#cd201f\" condition=\"progressAmount > 1 &amp;&amp; showProgressBlock\">\n    <!--  -->\n    <input type=\"range\" inline title=\"Progress 3\" name=\"progress3\" min=\"0\" max=\"100\" step=\"1\" value=\"80\" condition=\"progressAmount > 2 &amp;&amp; showProgressBlock\">\n    <input type=\"color\" title=\"Progress 3 Color\" name=\"progress3Color\" value=\"#cd201f\" condition=\"progressAmount > 2 &amp;&a